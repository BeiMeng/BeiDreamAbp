@{
    Layout = null;
}
<html>
<head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta name="renderer" content="webkit">
    <meta http-equiv="Cache-Control" content="no-siteapp" />
    <title>BeiDream后台管理系统</title>

    <meta name="keywords" content="">
    <meta name="description" content="">

    <!--[if lt IE 9]>
    <meta http-equiv="refresh" content="0;ie.html" />
    <![endif]-->
    <link href="~/Content/bootstrap.min.css" rel="stylesheet" />
    <link href="~/Areas/Admin/Content/font-awesome.min93e3.css" rel="stylesheet" />
    <link href="~/Areas/Admin/Content/animate.min.css" rel="stylesheet" />
    <link href="~/Areas/Admin/Content/style.min862f.css" rel="stylesheet" />
</head>

<body class="fixed-sidebar full-height-layout gray-bg" style="overflow:hidden">
    <div id="wrapper">
    <!--左侧导航开始-->
        @Html.Action("Sidebar", "Layout", new { area = "Admin", activePageMenu = ViewBag.ActivePageMenu })
    <!--左侧导航结束-->

        <!--右侧部分开始-->
        <div id="page-wrapper" class="gray-bg dashbard-1">
            <div class="row border-bottom">
                <nav class="navbar navbar-static-top" role="navigation" style="margin-bottom: 0">
                    <div class="navbar-header">
                        <a class="navbar-minimalize minimalize-styl-2 btn btn-primary " href="#"><i class="fa fa-bars"></i> </a>
                        @Html.Action("PageMenu", "Layout", new { area = "Admin", activePageMenu = ViewBag.ActivePageMenu })
                    </div>
                    @Html.Action("NotificationMenu", "Layout", new { area = "Admin" })
                </nav>
            </div>
            <div class="row content-tabs">
                <button class="roll-nav roll-left J_tabLeft">
                    <i class="fa fa-backward"></i>
                </button>
                <nav class="page-tabs J_menuTabs">
                    <div class="page-tabs-content">
                        <a href="javascript:;" class="active J_menuTab" data-id="index_v1.html">首页</a>
                    </div>
                </nav>
                <button class="roll-nav roll-right J_tabRight">
                    <i class="fa fa-forward"></i>
                </button>
                <div class="btn-group roll-nav roll-right">
                    <button class="dropdown J_tabClose" data-toggle="dropdown">
                        关闭操作<span class="caret"></span>

                    </button>
                    <ul role="menu" class="dropdown-menu dropdown-menu-right">
                        <li class="J_tabShowActive">
                            <a>定位当前选项卡</a>
                        </li>
                        <li class="divider"></li>
                        <li class="J_tabCloseAll">
                            <a>关闭全部选项卡</a>
                        </li>
                        <li class="J_tabCloseOther">
                            <a>关闭其他选项卡</a>
                        </li>
                    </ul>
                </div>
                <a href="login.html" class="roll-nav roll-right J_tabExit"><i class="fa fa fa-sign-out"></i> 退出</a>
            </div>
            <div class="row J_mainContent" id="content-main">
                <iframe class="J_iframe" name="iframe0" width="100%" height="100%" src="@Url.Action("Index", "Dashboard", new { area = "Admin" })" frameborder="0" data-id="index_v1.html" seamless></iframe>
            </div>
            <div class="footer">
                <div class="pull-center">
                    &copy; 2014-2015 <a href="https://github.com/BeiMeng" target="_blank">BeiDream</a>
                </div>
            </div>
        </div>
        <!--右侧部分结束-->

    <!--右侧边栏开始-->
        @Html.Action("RightSidebar", "Layout", new { area = "Admin" })
    <!--右侧边栏结束-->
    </div>
    <script src="~/Scripts/jquery-1.9.1.min.js"></script>
    <script src="~/Scripts/bootstrap.min.js"></script>
    <script src="~/Areas/Admin/Scripts/plugins/metisMenu/jquery.metisMenu.js"></script>
    <script src="~/Areas/Admin/Scripts/plugins/slimscroll/jquery.slimscroll.min.js"></script>
    <script src="~/Areas/Admin/Scripts/plugins/layer/layer.min.js"></script>
    <script src="~/Areas/Admin/Scripts/hplus.min862f.js"></script>
    <script src="~/Areas/Admin/Scripts/contabs.min.js"></script>
    <script src="~/Areas/Admin/Scripts/plugins/pace/pace.min.js"></script>
</body>
</html>