@model IEnumerable<BeiDream.MetronicMpa.Areas.Admin.Models.Layout.PageMenuViewModel>  
<div class="top-menu" style="float: left">
    <ul class="nav navbar-nav pull-left pageMenu">
        @foreach (var pageMenu in Model)
        {
            <li class="dropdown dropdown-extended @(pageMenu.IsActive ? "pageMenuActive" : "")">
                <a href="@Url.Action("Index", "Dashboard", new { area = "Admin", activePageMenu = pageMenu.PageName})">
                    <i class="@pageMenu.Icon"></i>
                    <span>@pageMenu.DisplayName</span>
                </a>
           </li>
        }
</ul>
</div>

